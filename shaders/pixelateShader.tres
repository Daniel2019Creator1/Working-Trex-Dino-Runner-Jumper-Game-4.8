[gd_resource type="Shader" format=2]

[resource]

code = "shader_type canvas_item;

uniform float frequency=100;
uniform float depth = 0.01;
uniform float foo = 0.001;
uniform float size_x=0.008;
uniform float size_y=0.008;

void fragment() {
	
	
	vec2 uv = SCREEN_UV;
	uv-=mod(uv,vec2(size_x,size_y));
	//uv.x += sin(uv.y*frequency+TIME)*uv.y*depth/3.0;
	uv.x = uv.x;
	uv.x = clamp(uv.x,0,1);
	vec3 c = textureLod(SCREEN_TEXTURE,uv,0.0).rgb;
	//c=vec3(1.0)-c;
	
	COLOR.rgb=c;
}
"

