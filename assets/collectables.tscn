void fragment() {
	vec2 uv = UV;
	//float alpha = (sin(TIME*speed)+1.0) / 2.0 * COLOR.a;
	vec4 maskTexture = texture(texBackground,uv);
	COLOR = textureLod(TEXTURE,uv,0.0);
	vec4 c;
	
	if (COLOR.a > 0.0) {
		c = COLOR * maskTexture;
	} else {
		if (maskTexture.r < 0.1) {
			c.a = 1.0;
		}
		c.a = 0.0;
	}
	COLOR.rgba = vec4(c.r,c.g,c.b,c.a);
}
